{
  "name": "dsi-iepg/cas-connection",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "description": "Mise en place d'une authentification par CAS",
  "keywords": [
    "cas", "casphp", "iepg"
  ],
  "homepage": "https://github.com/dsi-iepg/cas-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Vincent DELACOUR",
      "email": "vincent.delacour@iegp.fr",
      "homepage": "https://http://www.sciencespo-grenoble.fr/",
      "role": "Developer"
    }
  ],
    "require": {
        "php": ">=8.2",
        "apereo/phpcas": "^1.6",
        "symfony/security-bundle": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/config": "^7.0"
    },
  "autoload": {
    "psr-4": {
      "Iepg\\Bundle\\Cas\\": "/src"
    }
  },
  "extra": {
    "symfony": {
      "endpoint": [
        "https://raw.githubusercontent.com/symfony/recipes-contrib/flex/pull-1459/index.json",
        "flex://defaults"
      ]
    }
  }
}
